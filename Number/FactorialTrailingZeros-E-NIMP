//https://leetcode.com/problems/factorial-trailing-zeroes/description/?envType=study-plan-v2&envId=top-interview-150
//NIMP is not important to solve again
class Solution {
    public int trailingZeroes(int n) {
        int ans=0;
        int i=1;
        while(i<n){
        ans+=n/Math.pow(5,i);
        i++;
        }
        return ans;
    }
}
