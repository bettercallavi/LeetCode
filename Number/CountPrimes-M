//https://leetcode.com/problems/count-primes/description/
//https://en.wikipedia.org/wiki/Sieve_of_Eratosthenes

class Solution {
    public int countPrimes(int n) {
        boolean arr[]=new boolean[n];
        long c=0;
        for(long i=2;i<n;i++)
        {
            if(arr[(int)i])
             continue;
             c++;
            for(long j=i*i;j<n;j+=i)
             arr[(int)j]=true;
        }
        return (int)c;
    }
}
