//https://leetcode.com/problems/jump-game/description/


class Solution {
    public boolean canJump(int[] nums) {
        int maxHere=0;
        int maxLast=0;
        for(int i=0;i<nums.length;i++){
          maxHere=nums[i];
          maxLast=Math.max(maxHere,maxLast-1);
             System.out.println(maxLast);
          if(maxLast==0 && i<nums.length-1)  
          return false; 
        }
        return true;
    }
}
