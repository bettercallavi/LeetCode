//https://leetcode.com/problems/binary-tree-right-side-view/submissions/


class Solution {
    public List<Integer> rightSideView(TreeNode root) {
        List<Integer> list=new ArrayList<>();
        traverse(root,list,0);
        return list;
    }
    void  traverse(TreeNode node,List list,int h){
         if(node==null)
         return ;
         if(list.size()==h)
          list.add(node.val);
         traverse(node.right,list,h+1);
         traverse(node.left,list,h+1);
      }
}
