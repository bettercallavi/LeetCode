// not from leetcode 1->2->3->4
//store the next in begining only ,later u will loose 

static Node  rev(Node node){
        Node current=node;
        Node prv=null;
        Node next;
        while(current!=null){
            next=current.next;
            current.next=prv;
             prv=current;
            current=next;
        }
        return prv;
        }
